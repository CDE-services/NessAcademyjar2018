package shapes;

public class Rectangle extends Shape {
	private int width;
	private int height;
	
	public Rectangle(int width, int height) {
		this.width = width;
		this.height = height;
	}
	
	public Rectangle(int width, int height, int x, int y, char color) {
		super(x, y, color);
		this.width = width;
		this.height = height;
	}
	
	
}
